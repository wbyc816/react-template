{
  "name": "antd-demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.11.0",
    "axios": "^0.18.0",
    "babel-plugin-import": "^1.11.0",
    "query-string": "^6.2.0",
    "react": "^16.6.3",
    "react-app-rewire-less": "^2.1.3",
    "react-app-rewired": "^1.6.2",
    "react-dom": "^16.6.3",
    "react-intl": "^2.7.2",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "redux": "^4.0.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "react",
      "jsx-a11y"
    ],
    "rules": {
      "semi": 1,
      "react/jsx-first-prop-new-line": [
        2,
        "multiline"
      ],
      "react/jsx-closing-bracket-location": [
        2,
        "tag-aligned"
      ],
      "react/jsx-curly-spacing": [
        2,
        {
          "when": "never",
          "children": true
        }
      ],
      "react/jsx-indent-props": [
        2,
        2
      ],
      "react/jsx-indent": [
        2,
        2
      ],
      "react/jsx-max-props-per-line": [
        1,
        {
          "maximum": 1
        }
      ],
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "specialLink": [
            "hrefLeft",
            "hrefRight"
          ],
          "aspects": [
            "noHref",
            "preferButton"
          ]
        }
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "react-app-rewire-aliases": "^0.2.0"
  }
}
